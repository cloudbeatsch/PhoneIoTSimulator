<?xml version="1.0" encoding="utf-8"?>
<widget id="org.irjudson.simulator" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>IoT Simulator</name>
  <description>
    	A mobile application providing simulation data.
    </description>
  <author email="irjudson@gmail.com" href="http://irjudson.org">
    	Ivan R. Judson - Microsoft
    </author>
  <preference name="StatusBarStyle" value="lightcontent" />
  <preference name="BackupWebStorage" value="local" />
  <content src="index.html" />
  <access origin="*" />
  <vs:plugin name="cordova-plugin-device-motion" version="1.1.2-dev" src="https://github.com/apache/cordova-plugin-device-motion.git" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" />
  <vs:plugin name="cordova-plugin-device-orientation" version="1.0.2-dev" src="https://github.com/apache/cordova-plugin-device-orientation.git" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" />
</widget>